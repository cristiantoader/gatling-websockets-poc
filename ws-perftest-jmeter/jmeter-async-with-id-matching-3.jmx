          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="E2E Results JSR223 PostProcessor" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">def jsonSlurper = new groovy.json.JsonSlurper()

def response = prev.getResponseDataAsString();
log.info(&quot;Received response ${response}&quot;)

if (response) {
	def message = jsonSlurper.parseText(response);
	def messageId = message.msgId;
	log.info(&quot;Received response message id ${messageId}&quot;)
	
	def startTime = props.get(&quot;${messageId}&quot;) as Long;
	def endTime = System.currentTimeMillis() as Long;
	def delta = (endTime - startTime);
	
	prev.elapsedTime = delta
}

</stringProp>
          </JSR223PostProcessor>
