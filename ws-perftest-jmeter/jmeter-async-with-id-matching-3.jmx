        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">def connectionName = &quot;bds-ws-conn-${ctx.getThreadNum()}&quot;;
def connection = props.get(connectionName)

log.info(&quot;Using BDS WS writer connection ${connectionName} / ${connection}&quot;);
sampler.threadLocalCachedConnection.set(connection)

def msgId = new Random().nextInt(1_000_000)

// variable for the message sender
vars.put(&apos;msgId&apos;, &quot;user-${connectionName} msg-${msgId}&quot;);

log.info(&quot;Sending message ${msgId}&quot;)</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
